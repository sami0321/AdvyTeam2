@model IEnumerable<Domaine.employee>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.U_Cin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.U_FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.U_LastName)
        </th>
        
      
        <th>
            @Html.DisplayNameFor(model => model.U_PhoneNumber)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.U_Cin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.U_FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.U_LastName)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.U_PhoneNumber)
        </td>

        <td>         
            @Html.ActionLink("Details", "Details", new { id=item.U_ID })
        </td>
    </tr>
}

</table>
