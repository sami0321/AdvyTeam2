@model IEnumerable<Domaine.eval360>

@{
    ViewBag.Title = "Index";
}

<h2>Evaluation 360 degrées</h2>


<table class="table">
    <tr>
        <th>
            Liste des employés a evalué
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.employee.U_LastName)
        </td>
        <td>
            @Html.ActionLink("Evaluer", "evaluer", new { id = item.id }) |
            @Html.ActionLink("Details", "Details", new { id = item.id })
        </td>
    </tr>
}

</table>
